<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>KeepAlive</key>
  <true/>
  <key>Label</key>
  <string>{{ package_startup_script }}</string>
  <key>ProgramArguments</key>
  <array>
    <string>{{ package_dist_path }}/mailhog</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardErrorPath</key>
  <string>{{ package_log_path }}/mailhog.log</string>
  <key>StandardOutPath</key>
  <string>{{ package_log_path }}/mailhog.log</string>
</dict>
</plist>